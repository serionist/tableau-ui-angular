{
  "name": "tableau-component-library",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "downloadFonts": "node projects/component-library/styles/fonts/downloadFonts.js",
    "start": "ng serve test-app",
    "build": "ng build",
    "build:github-pages":"ng build test-app --base-href /tableau-ui-angular/",
